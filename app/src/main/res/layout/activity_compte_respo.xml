<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile"
    tools:context=".activities.gestion_comptes.gestion_respo.activity_profile_Respo"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="72dp">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="137dp"
        android:layout_height="120dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="289dp"
        android:layout_x="125dp"
        android:layout_y="17dp"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/editer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/av" />


    <TextView
        android:id="@+id/textView24"
        android:layout_width="171dp"
        android:layout_height="23dp"
        android:layout_marginBottom="418dp"
        android:layout_x="106dp"
        android:layout_y="154dp"
        android:text="Responsable de stage"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editRespoSave"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/editer"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:layout_marginTop="166dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="54dp"
        android:layout_x="344dp"
        android:layout_y="179dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/nomRes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icons8_edit_100px_1"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/editRespoSave"
        android:layout_width="199dp"
        android:layout_height="52dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="37dp"
        android:layout_x="103dp"
        android:layout_y="673dp"
        android:background="@color/teal_700"
        android:text="Sauvegarder"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView24"
        tools:ignore="TextContrastCheck" />

    <LinearLayout
        android:layout_width="348dp"
        android:layout_height="wrap_content"
        android:layout_x="26dp"
        android:layout_y="310dp"
        android:orientation="vertical">

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:outlineAmbientShadowColor="@color/purple_200">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/prenomRes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="bottom"
                android:hint="prÃ©nom"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textColorHint="#122FCF"
                android:textSize="18sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/nomRes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:ems="10"
                android:gravity="bottom"
                android:hint="nom"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:textColorHint="#122FCF"
                android:textSize="18sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/emailRes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:ems="10"
                android:gravity="bottom"
                android:hint="email"
                android:inputType="textEmailAddress"
                android:minHeight="48dp"
                android:textColorHint="#122FCF"
                android:textSize="18sp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:passwordToggleEnabled="true">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/passwordRes"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity="bottom"
                android:hint="mot de passe"
                android:inputType="textPassword"
                android:minHeight="48dp"
                android:textColorHint="#122FCF"
                android:textSize="18sp" />
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/ID_RESPO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="174dp"
        android:layout_y="183dp"
        android:text="ID_RESPO"
        android:textSize="16dp" />

</AbsoluteLayout>