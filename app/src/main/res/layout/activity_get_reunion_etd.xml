<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.gestion_comptes.gestion_etd.activity_get_reunion_etd"
    android:background="@drawable/input">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="328dp"
        android:layout_height="107dp"
        android:layout_x="43dp"
        android:layout_y="20dp"
        android:gravity="center"
        android:text="voir ma reunion"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="35sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="347dp"
        android:layout_height="313dp"
        android:layout_x="31dp"
        android:layout_y="269dp"
        android:orientation="vertical">

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:layout_marginBottom="30dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/get_etd_libelle_reunion"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:hint="Libelle de la reunion" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_marginBottom="30dp"

            android:layout_height="73dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/get_etd_date_reunion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Date de la reunion" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:layout_width="match_parent"
            android:layout_height="73dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/get_etd_salle_reunion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Salle" />
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

</AbsoluteLayout>