<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/viewRespo"
        android:layout_width="402dp"
        android:layout_height="3dp"
        android:layout_marginTop="150dp"
        android:background="#C2C2C2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gv_etd_email"
        android:layout_width="166dp"
        android:layout_height="30dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="28dp"
        android:text="email"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gv_etd_ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="ID_etd"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/gv_etd_email"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gv_etd_email"
        app:layout_constraintTop_toBottomOf="@+id/gv_etd_email" />

    <ImageView
        android:id="@+id/iv_etd_file"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:layout_marginStart="248dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icons8_file_200px"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/gv_etd_DateEnvoi"
        android:layout_width="105dp"
        android:layout_height="22dp"
        android:text="DateEnvoi"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gv_etd_email"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.534" />

    <TextView
        android:id="@+id/gv_etd_filename"
        android:layout_width="124dp"
        android:layout_height="38dp"
        android:text="fileName"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_etd_file"
        app:layout_constraintStart_toStartOf="@+id/iv_etd_file"
        app:layout_constraintTop_toBottomOf="@+id/iv_etd_file"
        app:layout_constraintVertical_bias="0.261" />
</androidx.constraintlayout.widget.ConstraintLayout>